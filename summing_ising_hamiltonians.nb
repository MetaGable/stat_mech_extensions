Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"PartitionFunctionForIsingSpins", "[", 
    RowBox[{"Hamiltonian_", ",", "n_", ",", 
     RowBox[{"OptionsPattern", "[", 
      RowBox[{"{", 
       RowBox[{"\"\<Verbose\>\"", "->", "False"}], "}"}], "]"}]}], "]"}], ":=", 
   RowBox[{"Module", "[", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"{", "\[IndentingNewLine]", 
      RowBox[{
      "spins", ",", "\[IndentingNewLine]", "PartitionFunction", ",", 
       "\[IndentingNewLine]", "sum", ",", "\[IndentingNewLine]", "i"}], 
      "\[IndentingNewLine]", "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
      "Define", " ", "spins", " ", "as", " ", "symbolic", " ", "variables", 
       " ", 
       RowBox[{"S", "[", "i", "]"}]}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"spins", "=", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"s", "[", "i", "]"}], ",", 
         RowBox[{"{", 
          RowBox[{"i", ",", "1", ",", "n"}], "}"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"(*", 
       RowBox[{
       "Perform", " ", "the", " ", "summation", " ", "over", " ", "all", " ", 
        "spin", " ", "configurations"}], "*)"}], "\[IndentingNewLine]", 
      RowBox[{"sum", "=", "0"}], ";", "\[IndentingNewLine]", 
      RowBox[{"Do", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"sum", "+=", 
          RowBox[{"Exp", "[", 
           RowBox[{
            RowBox[{
             RowBox[{"-", "\[Beta]"}], " ", "Hamiltonian"}], "/.", " ", 
            RowBox[{"Table", "[", 
             RowBox[{
              RowBox[{
               RowBox[{"s", "[", "k", "]"}], "->", 
               RowBox[{"spinConfig", "[", 
                RowBox[{"[", "k", "]"}], "]"}]}], ",", 
              RowBox[{"{", 
               RowBox[{"k", ",", "1", ",", "n"}], "}"}]}], "]"}]}], "]"}]}], 
         ";", "\[IndentingNewLine]", 
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{"OptionValue", "[", "\"\<Verbose\>\"", "]"}], ",", 
           "\[IndentingNewLine]", 
           RowBox[{"Print", "[", 
            RowBox[{"\"\<> Successfully summed over spin #\>\"", ",", "i"}], 
            "]"}]}], "\[IndentingNewLine]", "]"}], ";"}], ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", "1", ",", "n"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"spinConfig", ",", 
          RowBox[{"Tuples", "[", 
           RowBox[{
            RowBox[{"{", 
             RowBox[{
              RowBox[{"-", "1"}], ",", "1"}], "}"}], ",", "n"}], "]"}]}], 
         "}"}]}], "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
      RowBox[{"(*", 
       RowBox[{
       "Assign", " ", "final", " ", "partition", " ", "function", " ", 
        "value"}], "*)"}], "\[IndentingNewLine]", 
      RowBox[{"PartitionFunction", "=", "sum"}], ";", "\[IndentingNewLine]", 
      RowBox[{"(*", 
       RowBox[{"Return", " ", "the", " ", "partition", " ", "function"}], 
       "*)"}], "\[IndentingNewLine]", "PartitionFunction"}]}], "]"}]}], 
  "\[IndentingNewLine]"}]], "Input",
